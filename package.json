{
  "name": "@uniswap-v2-app/monorepo",
  "version": "1.0.0",
  "keywords": [
    "uniswap",
    "ethereum",
    "react",
    "workspaces",
    "yarn"
  ],
  "private": true,
  "scripts": {
    "vercel:install": "sudo yarn global add vercel@latest",
    "vercel:dev": "vercel dev -d",
    "vercel:prod": "vercel --prod",
    "react-app:build": "yarn workspace @uniswap-v2-app/react-app build",
    "react-app:eject": "yarn workspace @uniswap-v2-app/react-app eject",
    "react-app:ipfs": "yarn workspace @uniswap-v2-app/react-app ipfs",
    "react-app:start": "yarn workspace @uniswap-v2-app/react-app start",
    "react-app:test": "yarn workspace @uniswap-v2-app/react-app test"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "@sentry/browser": "^7.106.0"
  }
}
